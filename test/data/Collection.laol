// All objects are immutable (val) by default

class Array {
    public abstract def size =>int
    public abstract def empty?
    public abstract def each(->kv(ix, value))
    public abstract def [](ix)
    public abstract def <<(value)
    public abstract def []=(ix, value)
}

class Map {
    public abstract def size =>int
    public abstract def empty?
    public abstract def keys =>[]
    public abstract def values =>[]
    public abstract def each(->kv(key, value))
    public abstract def [](key)
    public abstract def []=(key, value)
}
